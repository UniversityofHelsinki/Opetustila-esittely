{
    "name": "Tilat Composer File",
    "description": "HY-Tilat Composer file",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "contrib/d8-termstatus",
                "version": "dev-master",
                "type":"drupal-module",
                "source": {
                    "url": "https://github.com/ainarend/D8-termstatus.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/console": "^1.0.2",
        "drupal/core": "8.6.9",
        "drush/drush": "~9.5.2",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "drupal/config_installer": "~1.0",
        "drupal/warden": "*",
        "drupal/simplei": "^1.0",
        "wunderio/drupal-ping": "^1.0",
        "drupal/admin_toolbar": "^1.23",
        "drupal/token": "^1.1",
        "drupal/pathauto": "^1.0",
        "drupal/config_split": "^1.3",
        "drupal/config_filter": "^1.1",
        "drupal/ctools": "^3.0",
        "drupal/webform": "^5.0@RC",
        "drupal/config_ignore": "^2.1",
        "drupal/migrate_tools": "4.0-beta3",
        "drupal/migrate_plus": "4.0-beta3",
        "drupal/libraries": "^3.0@alpha",
        "drupal/field_permissions": "^1.0@beta",
        "drupal/field_group": "3.0-beta1",
        "drupal/ds": "3.1",
        "drupal/ds_chains": "^1.0@alpha",
        "drupal/search_api": "^1.7",
        "drupal/search_api_autocomplete": "1.0-rc1",
        "drupal/disable_field": "^1.8",
        "drupal/taxonomy_publish": "^1.0",
        "drupal/field_group_link": "^3.0@RC",
        "drupal/easy_breadcrumb": "^1.6",
        "drupal/leaflet": "^1.0@beta",
        "drupal/redirect": "^1.1",
        "drupal/geofield_map": "^1.44",
        "drupal/permissions_by_term": "^1.44",
        "contrib/d8-termstatus": "*",
        "drupal/google_tag": "^1.0",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drupal/cookieconsent": "^1.4"
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "codeception/visualception": "^0.9.0",
        "guncha25/codecept-drupal-drush": "~0.3",
        "guncha25/drupal-8-bootstrap": "0.3",
        "drupal/devel": "~2.0",
        "drupal/devel_php": "^1.0",
        "drupal/stage_file_proxy": "^1.0@alpha"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "dropin-paths": {
            "web/": ["type:web-dropin"]
        },
        "patches": {
          "drupal/search_api_autocomplete": {
              "Fix uppercase search autocomplete": "https://www.drupal.org/files/issues/autocomplete-without-case-sensitive-2947273-4.patch"
          }
        }
    }
}
